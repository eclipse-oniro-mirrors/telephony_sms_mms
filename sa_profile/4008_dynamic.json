{
    "process": "telephony",
    "systemability": [
        {
            "name": 4008,
            "libpath": "libtel_sms_mms.z.so",
            "run-on-create": false,
            "depend": [
                4010
            ],
            "depend_time_out": 60000,
            "distributed": false,
            "dump_level": 1,
            "start-on-demand":{
                "param":[
                    {
                        "eventId":"param",
                        "name":"const.vendor.ril.dynamic_switch_modem",
                        "value":"false"
                    }
                ],
                "param":[
                    {
                        "name":"telephony.dynamic_poweroff_modem",
                        "value":"0",
                        "conditions":[
                            {
                                "eventId":"param",
                                "name":"const.vendor.ril.dynamic_switch_modem",
                                "value":"true"
                            }
                        ]
                    }
                ]
            },
            "stop-on-demand":{
                "param":[
                    {
                        "eventId":"param",
                        "name":"const.vendor.ril.dynamic_switch_modem",
                        "value":"true"
                    }
                ],
                "param":[
                    {
                        "name":"telephony.dynamic_poweroff_modem",
                        "value":"1",
                        "conditions":[
                            {
                                "eventId":"param",
                                "name":"const.vendor.ril.dynamic_switch_modem",
                                "value":"true"
                            }
                        ]
                    }
                ]
            }
        }
    ]
}